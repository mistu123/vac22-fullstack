{
  "name": "expense-tracker",
  "version": "1.0.0",
  "contributors": [
    "Tamaghna Banerjee"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4590",
    "dev": "ng serve --port 4590 --configuration=hmr --watch",
    "build-dev": "ng build --base-href ./index.html --configuration=dev",
    "build-stage": "ng build --base-href ./index.html --configuration=stage",
    "build-prod": "ng build --base-href ./index.html --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/pwa": "^12.1.2",
    "@angular/router": "~13.3.11",
    "@angular/service-worker": "~13.3.11",
    "@angularclass/hmr": "^3.0.0",
    "@ngx-loading-bar/core": "^6.0.2",
    "angular-dark-mode": "^3.0.2",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "jwt-decode": "^3.1.2",
    "lottie-web": "^5.9.6",
    "moment": "^2.29.4",
    "ngx-bootstrap": "^8.0.0",
    "ngx-lottie": "^8.2.0",
    "rxjs": "~6.6.0",
    "toastify-js": "^1.12.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular-eslint/builder": "13.3.0",
    "@angular-eslint/eslint-plugin": "13.3.0",
    "@angular-eslint/eslint-plugin-template": "13.3.0",
    "@angular-eslint/schematics": "^13.3.0",
    "@angular-eslint/template-parser": "13.3.0",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.17.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.1.1",
    "prettier": "2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "~4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint && lint-staged",
      "pre-push": ""
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,css,scss,md,html,json}": [
      "prettier --write"
    ]
  }
}
